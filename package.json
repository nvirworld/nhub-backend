{
  "name": "nhub-backend",
  "version": "0.0.1",
  "engines": {
    "node": "18.16"
  },
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typechain": "typechain --target ethers-v6 src/common/abi/*.abi.json --out-dir src/typechain-types",
    "build": "yarn typechain && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "yarn typechain && AWS_SECRET=nhub-secret-backend-prod AWS_PROFILE=nhub NODE_ENV=local nest start --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@adminjs/express": "^5.1.0",
    "@adminjs/nestjs": "^5.1.1",
    "@adminjs/passwords": "^3.0.1",
    "@adminjs/typeorm": "^4.0.0",
    "@aws-sdk/client-secrets-manager": "^3.370.0",
    "@aws-sdk/client-sqs": "^3.370.0",
    "@aws-sdk/client-sts": "^3.370.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@slack/webhook": "^7.0.1",
    "@ssut/nestjs-sqs": "^2.2.0",
    "@tiptap/core": "^2.0.4",
    "@tiptap/pm": "^2.0.4",
    "adminjs": "^6.8.7",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "ethers": "^6.6.5",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "fast-safe-stringify": "^2.1.1",
    "fastq": "^1.15.0",
    "logform": "^2.5.1",
    "mysql2": "^3.5.2",
    "node-telegram-bot-api": "^0.64.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@typechain/ethers-v6": "^0.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^3.2.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typechain": "^8.3.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
